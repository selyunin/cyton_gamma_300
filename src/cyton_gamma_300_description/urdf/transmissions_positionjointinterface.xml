<?xml version="1.0"?>
<robot name="cyton_gamma_300" xmlns:xacro="http://www.ros.org/wiki/xacro">


  <!-- ########################################### -->
  <!-- #! Shoulder Roll: Transmission Interface !# -->
  <!-- ########################################### -->
  <transmission name="trans_shoulder_roll">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="shoulder_roll_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="shoulder_roll_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

