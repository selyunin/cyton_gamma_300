<launch>

  <node name="cyton_controller_manager" pkg="dynamixel_controllers"
	type="controller_manager.py" ns="/cyton_gamma_300" required="true" output="screen">
    <rosparam>
      namespace: cyton_manager
      serial_ports:
        port_0:
          port_name: "/dev/ttyUSB0"
          baud_rate: 1000000
          min_motor_id: 0
          max_motor_id: 6
          update_rate: 20
    </rosparam>
  </node>

</launch>
